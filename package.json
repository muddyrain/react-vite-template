{
  "name": "react-vite-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run server & vite",
    "server": "node server/index.cjs",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": " eslint \"src/**/*.+(js|ts|jsx|tsx)\"",
    "format": "npx prettier --write \"src/**/*.+(js|ts|jsx|tsx)\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@gltf-transform/functions": "^3.2.1",
    "@react-three/cannon": "^6.5.2",
    "@types/mockjs": "^1.0.7",
    "@types/node": "^18.16.2",
    "@typescript-eslint/eslint-plugin": "^5.40.0",
    "@typescript-eslint/parser": "^5.40.0",
    "dayjs": "^1.11.7",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "gsap": "^3.11.5",
    "koa": "^2.14.2",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "leva": "^0.9.34",
    "mockjs": "^1.1.0",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-transition-group": "^4.4.5",
    "react-tsparticles": "^2.9.3",
    "three-stdlib": "^2.21.8",
    "tsparticles": "^2.9.3",
    "zustand": "^4.3.7"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@bees/ui": "git+https://gitee.com/public-cli/bees-ui.git",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@react-three/drei": "^9.65.5",
    "@react-three/fiber": "^8.12.2",
    "@types/axios": "^0.14.0",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/three": "^0.150.2",
    "@vitejs/plugin-react": "^2.1.0",
    "ahooks": "^3.7.2",
    "antd": "^5.1.5",
    "autoprefixer": "^10.4.13",
    "axios": "^1.1.2",
    "babel-loader": "^8.2.5",
    "cz-customizable": "^6.3.0",
    "eslint-plugin-storybook": "^0.6.4",
    "husky": "^7.0.1",
    "less": "^4.1.3",
    "moment": "^2.29.4",
    "postcss": "^8.4.18",
    "react-router-dom": "^6.4.2",
    "tailwindcss": "^3.2.1",
    "three": "^0.151.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^3.1.0",
    "vite-plugin-gltf": "^3.1.0",
    "vite-plugin-mock": "^2.9.6"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./.cz-config.cjs"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  }
}